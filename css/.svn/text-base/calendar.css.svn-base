/*Calendar Styles*/
#calendar_container {
    color: #9C9EA1;
    float: right;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 20px;
    margin-right: 10px;
    width: 24%;
}
#calendar_header {
    overflow: hidden;
    margin-bottom: 19px;
}
#calendar_category {
    float: left;
}
#calendar_menu {
    float: right;
    overflow: hidden;
}
#calendar_menu a {
    display: block;
    float: right;
    height: 22px;
    margin-left: 10px;
    width: 22px;
    margin-top: 6px;
}
/*
#calendar_menu a.calendar-monthly {
